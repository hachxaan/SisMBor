<+>{% extends 'body.html' %}\n{% load static %}\n\n{% block stylesheet %}\n    {% include '../MainSections/CSS-DataTables.html' %}\n    <link rel=\"stylesheet\" href=\"{% static 'sismbor/css/orden-editar.css' %}\">\n{% endblock %}\n\n{% block contento %}\n\n    <div id=\"data_from_django\" class=\"d-none\">{{ url_prev }}</div>\n    <div class=\"col-12\">\n        <div class=\"card card-primary card-outline card-outline-tabs\">\n            <div class=\"card-header p-0 border-bottom-0 pt-1\">\n                <div class=\"row mb-1 pl-1\">\n                    <div class=\"input-group col-lg-2 pr-3 h-50\">\n                        <a href=\"{{ url_prev }}\" class=\"pt-4 btn btn-secondary \"> <i\n                                class=\"fas fa-backward\"> Regresar</i></a>\n                        <span class=\"status badge badge-{{ status_tipo }}\">{{status_tipo}}{{ status_desc }}</span>\n                    </div>\n\n                    <div class=\"input-group col-lg-2 mb-3 mt-3 ml-2\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-secondary\">Placa</span>\n                        </div>\n                        <input type=\"text\" class=\"form-control uppercase\" readonly\n                               value=\"{{ placa }}\">\n                    </div>\n                    <div class=\"input-group col-lg-2 mb-3 mt-3\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-secondary\">Km</span>\n                        </div>\n                        <input type=\"text\" class=\"form-control uppercase\" readonly\n                               value=\"{{ kilometraje }}\">\n                    </div>\n                    <div class=\"input-group col-lg-2 mb-3 mt-3\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-secondary\">Modelo</span>\n                        </div>\n                        <input type=\"text\" class=\"form-control uppercase\" readonly\n                               value=\"{{ modelo }}\">\n                    </div>\n\n                </div>\n\n                <ul class=\"nav nav-tabs\" id=\"custom-tabs-four-tab\" role=\"tablist\">\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link active\" id=\"custom-tabs-four-repuestos-tab\" data-toggle=\"pill\"\n                           href=\"#custom-tabs-four-repuestos\"\n                           role=\"tab\" aria-controls=\"custom-tabs-four-repuestos\" aria-selected=\"false\">R e p u e s t o\n                            s</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link\" id=\"custom-tabs-four-mantenimiento-tab\" data-toggle=\"pill\"\n                           href=\"#custom-tabs-four-mantenimiento\" role=\"tab\" aria-controls=\"custom-tabs-four-profile\"\n                           aria-selected=\"false\">M a n t e n i e m i e n t o</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link\" id=\"custom-tabs-four-manobra-tab\" data-toggle=\"pill\"\n                           href=\"#custom-tabs-four-manobra\" role=\"tab\" aria-controls=\"custom-tabs-four-messages\"\n                           aria-selected=\"true\">M a n o d e O b r a</a>\n                    </li>\n                    {% comment %}   <li class=\"nav-item\">\n                        <a class=\"nav-link\" id=\"custom-tabs-four-neumaticos-tab\" data-toggle=\"pill\"\n                           href=\"#custom-tab-four-neumaticos\" role=\"tab\" aria-controls=\"custom-tabs-four-settings\"\n                           aria-selected=\"false\">Neumáticos</a>\n                    </li>{% endcomment %}\n                </ul>\n            </div>\n            <div class=\"card-body\">\n                <div class=\"tab-content\" id=\"custom-tabs-four-tabContent\">\n                    <div class=\"tab-pane fade active show\" id=\"custom-tabs-four-repuestos\" role=\"tabpanel\"\n                         aria-labelledby=\"custom-tabs-four-repuestos-tab\">\n                        {# ************************************************************************************************ #}\n                        {# REPUESTOS TABLA #}\n                        {# ************************************************************************************************ #}\n                        <table class=\"table table-bordered display\" style=\"width:100%\" id=\"dtRepuestos\">\n                            <thead>\n                            <tr>\n                                <th scope=\"col\" style=\"width: 5%;\">ID</th>\n                                <th scope=\"col\" style=\"width: 7%;\">Clave</th>\n\n\n                                <th scope=\"col\" style=\"width: 25%;\">Descripción</th>\n                                <th scope=\"col\" style=\"width: 3%;\">Precio</th>\n                                <th scope=\"col\" style=\"width: 8%;\">Marca</th>\n\n                                <th scope=\"col\" style=\"width: 8%;\">Categoría</th>\n                                <th scope=\"col\" style=\"width: 2%;\">U/Med.</th>\n                                <th scope=\"col\" style=\"width: 8%;\">V/U Km</th>\n                                <th scope=\"col\" style=\"width: 8%;\">V/U Hr</th>\n                                <th scope=\"col\" style=\"width: 8%;\">Periodo</th>\n                                <th scope=\"col\" style=\"width: 2%;\">Stock</th>\n                                <th scope=\"col\" style=\"width: 5%;\">Carrito</th>\n\n                            </tr>\n                            </thead>\n                        </table>\n                        {# ************************************************************************************************ #}\n                    </div>\n                    <div class=\"tab-pane fade\" id=\"custom-tabs-four-mantenimiento\" role=\"tabpanel\"\n                         aria-labelledby=\"custom-tabs-four-mantenimiento-tab\">\n                        {# ************************************************************************************************ #}\n                        {# REPUESTOS TABLA #}\n                        {# ************************************************************************************************ #}\n                        <table class=\"table table-bordered display\" style=\"width:100%\" id=\"dtMantenimiento\">\n                            <thead>\n                            <tr>\n                                <th scope=\"col\" style=\"width: 5%;\">ID</th>\n                                <th scope=\"col\" style=\"width: 7%;\">Clave</th>\n                                <th scope=\"col\" style=\"width: 25%;\">Descripción</th>\n                                <th scope=\"col\" style=\"width: 3%;\">Precio</th>\n                                <th scope=\"col\" style=\"width: 8%;\">Hora hombre</th>\n                                <th scope=\"col\" style=\"width: 5%;\">Carrito</th>\n\n                            </tr>\n                            </thead>\n                        </table>\n                        {# ************************************************************************************************ #}\n                    </div>\n                    <div class=\"tab-pane fade\" id=\"custom-tabs-four-manobra\" role=\"tabpanel\"\n                         aria-labelledby=\"custom-tabs-four-manobra-tab\">\n                        {# ************************************************************************************************ #}\n                        {# MANO DE OBRA TABLA #}\n                        {# ************************************************************************************************ #}\n                        <table class=\"table table-bordered display\" style=\"width:100%\" id=\"dtManoObra\">\n                            <thead>\n                            <tr>\n                                <th scope=\"col\" style=\"width: 5%;\">ID</th>\n                                <th scope=\"col\" style=\"width: 7%;\">Clave</th>\n                                <th scope=\"col\" style=\"width: 25%;\">Descripción</th>\n                                <th scope=\"col\" style=\"width: 3%;\">Precio</th>\n                                <th scope=\"col\" style=\"width: 8%;\">Hora hombre</th>\n                                <th scope=\"col\" style=\"width: 5%;\">Carrito</th>\n\n                            </tr>\n                            </thead>\n                        </table>\n                        {# ************************************************************************************************ #}\n                    </div>\n                    {% comment %}        <div class=\"tab-pane fade\" id=\"custom-tabs-four-settings\" role=\"tabpanel\"\n                         aria-labelledby=\"custom-tabs-four-settings-tab\">\n\n                    </div>{% endcomment %}\n                </div>\n            </div>\n            <!-- /.card -->\n        </div>\n\n    </div>\n    <div class=\"col-12\">\n        <div class=\"card card-primary card-outline card-outline-tabs\">\n            {# ************************************************************************************************ #}\n            {#  S E L E C C I O N   T A B L A  #}\n            {# ************************************************************************************************ #}\n            <table class=\"table table-bordered\" style=\"width:100%\" id=\"dtOrdenDetalle\">\n                <thead>\n                <tr>\n                    <th scope=\"col\" style=\"width: 5%;\">ID</th>\n                    <th scope=\"col\" style=\"width: 5%;\">Tipo</th>\n                    <th scope=\"col\" style=\"width: 5%;\">Cantidad</th>\n                    <th scope=\"col\" style=\"width: 7%;\">Categoria</th>\n                    <th scope=\"col\" style=\"width: 7%;\">Present.</th>\n                    <th scope=\"col\" style=\"width: 25%;\">Descripción</th>\n                    <th scope=\"col\" style=\"width: 10%;\">No. Serie</th>\n                    <th scope=\"col\" style=\"width: 15%;\">Encargado</th>\n                    <th scope=\"col\" style=\"width: 15%;\">Referencia</th>\n                    <th scope=\"col\" style=\"width: 5%;\">Acción</th>\n\n                </tr>\n                </thead>\n            </table>\n            {# ************************************************************************************************ #}\n        </div>\n\n    </div>\n\n    {% comment %}/*****************************************************************************************************/\n    /* TX REFERENCIA */\n    /**************************************************************************************************/{% endcomment %}\n\n\n    <div class=\"modal fade\" id=\"TxReferenciaModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"TxReferenciaLabel\"\n         aria-hidden=\"true\">\n        <input type=\"hidden\" name=\"id_orden_detalle\" id=\"id_orden_detalle\" value=\"0\">\n        <div class=\"modal-dialog\" role=\"document\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                    <h5 class=\"modal-title\" id=\"TxReferenciaLabel\">Referencia</h5>\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                        <span aria-hidden=\"true\">&times;</span>\n                    </button>\n                </div>\n                <div class=\"modal-body\">\n                    <form>\n                        <div class=\"form-group\">\n                            <label  class=\"col-form-label\">Edite la referencia para este\n                                item:</label>\n                            <input id=\"edtTxReferencia\" type=\"textarea\" class=\"form-control\">\n                        </div>\n                    </form>\n                </div>\n                <div class=\"modal-footer\">\n                    <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Cancelar</button>\n                    <button id=\"btnGuardarTxReferencia\" type=\"button\" class=\"btn btn-primary\">Guardar</button>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    {% comment %}/*****************************************************************************************************/\n    /* NO SERIE */\n    /**************************************************************************************************/{% endcomment %}\n\n    <div class=\"modal fade\" id=\"NoSerieModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"NoSerieLabel\"\n         aria-hidden=\"true\">\n        <input type=\"hidden\" name=\"id_orden_detalle\" id=\"id_orden_detalle\" value=\"0\">\n        <div class=\"modal-dialog\" role=\"document\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                    <h5 class=\"modal-title\" id=\"NoSerieLabel\">Número de Serie</h5>\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                        <span aria-hidden=\"true\">&times;</span>\n                    </button>\n                </div>\n                <div class=\"modal-body\">\n                    <form>\n                        <div class=\"form-group\">\n                            <label class=\"col-form-label\">Edite el número de serie para este\n                                item:</label>\n                            <input id=\"edtNoSerie\" type=\"textarea\" class=\"form-control\">\n                        </div>\n                    </form>\n                </div>\n                <div class=\"modal-footer\">\n                    <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Cancelar</button>\n                    <button id=\"btnGuardarNoSerie\" type=\"button\" class=\"btn btn-primary\">Guardar</button>\n                </div>\n            </div>\n        </div>\n    </div>\n\n\n    {% comment %}/*****************************************************************************************************/\n    /* PERSONAL */\n    /**************************************************************************************************/{% endcomment %}\n\n    <div class=\"modal fade w-50\" id=\"PersonalModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"PersonalLabel\"\n         aria-hidden=\"true\">\n        <input type=\"hidden\" name=\"id_orden_detalle\" id=\"id_orden_detalle\" value=\"0\">\n        <div class=\"modal-dialog\" role=\"document\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                    <h5 class=\"modal-title\" id=\"PersonalLabel\">Persona encargada</h5>\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                        <span aria-hidden=\"true\">&times;</span>\n                    </button>\n                </div>\n                <div class=\"modal-body\">\n                    <form>\n                        <div class=\"form-group\">\n                            <label class=\"col-form-label\">Seleccione a la parsona que se encargará\n                                del trabajo:</label>\n                            <select class=\"select2 w-100\" name='personal' id='cbPersonal'>\n                                {% for persona in personal %}\n                                    <option value=\"{{ persona.id_personal }}\">{{ persona.nombre }} {{ persona.nombre }}</option>\n                                {% endfor %}\n                            </select>\n                        </div>\n                    </form>\n                </div>\n                <div class=\"modal-footer\">\n                    <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Cancelar</button>\n                    <button id=\"btnGuardarPersonal\" type=\"button\" class=\"btn btn-primary\">Guardar</button>\n                </div>\n            </div>\n        </div>\n    </div>\n\n\n{% endblock %}\n\n\n{% block scripts %}\n    {% include '../MainSections/JS-DataTables.html' %}\n    <script src=\"{% static 'sismbor/js/orden-editar.js' %}\" type=\"text/javascript\"></script>\n{% endblock %}
